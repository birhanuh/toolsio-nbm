{
  "name": "toolsio-server",
  "version": "1.0.0",
  "description": "A Project tracking and Sales management tool.",
  "main": "./src/index.js",
  "directories": {
    "test": "./src/__test__"
  },
  "scripts": {
    "serve": "NODE_ENV=production node dist/index.js",
    "build": "babel src -d dist",
    "postbuild": "cp -r ./src/types ./dist",
    "start": "NODE_ENV=development nodemon --exec babel-node -- src/index.js",
    "test:server": "NODE_ENV=test npm run db:reset && NODE_ENV=test npm run db:migrate && NODE_ENV=test babel-node -- src/index.js",
    "db:reset": "./__test__/helpers/reset_db.sh",
    "migration:create": "./node_modules/.bin/sequelize migration:create --name",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed:generate": "./node_modules/.bin/sequelize seed:generate --name",
    "db:seed": "./node_modules/.bin/sequelize db:seed --seed",
    "db:seed:all": "./node_modules/.bin/sequelize db:seed:all",
    "pretest": "NODE_ENV=test npm run db:reset && NODE_ENV=test npm run db:migrate",
    "test": "NODE_ENV=test jest --runInBand --forceExit './__test__' "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birhanuh/toolsio-react.git"
  },
  "author": "Birhanu Hailemariam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/birhanuh/toolsio-react/issues"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/birhanuh/toolsio-node#readme",
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "aws-sdk": "^2.382.0",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "cloudinary": "^1.13.2",
    "connect-pg-simple": "^5.0.0",
    "connect-redis": "^3.4.0",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^6.2.0",
    "email-templates": "^5.0.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-sessions": "^1.0.6",
    "formidable": "^1.2.1",
    "graphql": "^14.0.2",
    "graphql-redis-subscriptions": "^2.0.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.3",
    "ioredis": "^4.3.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "nodemailer": "^4.7.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemailer-sparkpost-transport": "^2.2.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "pug": "^2.0.3",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "shortid": "^2.2.14",
    "socket.io": "^2.2.0",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.10.0",
    "faker": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-nodemon": "^2.4.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "phantomjs": "^2.1.7"
  }
}
