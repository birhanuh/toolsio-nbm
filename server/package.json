{
  "name": "toolsio-server",
  "version": "1.0.0",
  "description": "A Project tracking and Sales management tool.",
  "main": "./src/index.js",
  "directories": {
    "test": "./src/__test__"
  },
  "scripts": {
    "serve": "NODE_ENV=production node dist/index.js",
    "build": "babel src -d dist",
    "start": "NODE_ENV=development nodemon --exec babel-node -- src/index.js",
    "tstart": "NODE_ENV=test npm run db:reset && NODE_ENV=test npm run db:migrate && NODE_ENV=test babel-node -- src/index.js",
    "db:reset": "./__test__/helpers/reset_db.sh",
    "migration:create": "./node_modules/.bin/sequelize migration:create --name",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed:generate": "./node_modules/.bin/sequelize seed:generate --name",
    "db:seed": "./node_modules/.bin/sequelize db:seed --seed",
    "db:seed:all": "./node_modules/.bin/sequelize db:seed:all",
    "pretest": "NODE_ENV=test npm run db:reset && NODE_ENV=test npm run db:migrate",
    "test": "NODE_ENV=test jest --runInBand './__test__' "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birhanuh/toolsio-react.git"
  },
  "author": "Birhanu Hailemariam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/birhanuh/toolsio-react/issues"
  },
  "homepage": "https://github.com/birhanuh/toolsio-node#readme",
  "dependencies": {
    "apollo-server-express": "^1.3.4",
    "apollo-upload-server": "^5.0.0",
    "aws-sdk": "^2.220.1",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cloudinary": "^1.11.0",
    "connect-pg-simple": "^4.2.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "dotenv": "^6.0.0",
    "email-templates": "^4.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-sessions": "^1.0.6",
    "formidable": "^1.2.1",
    "graphql": "^0.13.2",
    "graphql-redis-subscriptions": "^1.5.0",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^2.23.1",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "merge-graphql-schemas": "^1.5.1",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.0.12",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "pug": "^2.0.3",
    "sequelize": "^4.37.5",
    "sequelize-cli": "^4.0.0",
    "shortid": "^2.2.8",
