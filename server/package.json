{
  "name": "toolsio-server",
  "version": "1.0.0",
  "description": "A Project tracking and Sales management tool.",
  "main": "./src/index.js",
  "directories": {
    "test": "./src/__test__"
  },
  "scripts": {
    "serve": "NODE_ENV=production node dist/index.js",
    "build": "babel src -d dist",
    "build:test": "NODE_ENV=test babel src -d dist && cp -r ./src/types ./dist",
    "postbuild": "cp -r ./src/types ./dist",
    "start": "NODE_ENV=development nodemon --exec babel-node -- src/index.js",
    "test:server": "NODE_ENV=test npm run db:reset && NODE_ENV=test npm run db:migrate && NODE_ENV=test babel-node -- src/index.js",
    "db:reset": "./__test__/helpers/reset_db.sh",
    "migration:create": "./node_modules/.bin/sequelize migration:create --name",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed:generate": "./node_modules/.bin/sequelize seed:generate --name",
    "db:seed": "./node_modules/.bin/sequelize db:seed --seed",
    "db:seed:all": "./node_modules/.bin/sequelize db:seed:all",
    "pretest": "NODE_ENV=test npm run db:reset && NODE_ENV=test npm run db:migrate",
    "jest": "./node_modules/.bin/jest --detectOpenHandles",
    "test": "NODE_ENV=test jest --runInBand --forceExit './__test__' "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birhanuh/toolsio-react.git"
  },
  "author": "Birhanu Hailemariam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/birhanuh/toolsio-react/issues"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/birhanuh/toolsio-node#readme",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@google-cloud/storage": "^2.4.3",
    "acorn": "^6.1.1",
    "apollo-server-express": "^2.4.8",
    "aws-sdk": "^2.431.0",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "cloudinary": "^1.14.0",
    "connect-pg-simple": "^5.0.0",
    "connect-redis": "^3.4.1",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^7.0.0",
    "email-templates": "^5.0.4",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-sessions": "^1.0.6",
    "formidable": "^1.2.1",
    "graphql": "^14.2.0",
    "graphql-redis-subscriptions": "^2.1.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.4",
    "ioredis": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "nodemailer": "^6.0.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemailer-sparkpost-transport": "^2.2.0",
    "passport-local": "^1.0.0",
    "pg": "^7.9.0",
    "pug": "^2.0.3",
    "sequelize": "^4.44.0",
    "sequelize-cli": "^5.5.0",
    "shortid": "^2.2.14",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "eslint": "^5.15.3",
    "faker": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-nodemon": "^2.4.2",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "nodemon": "^1.18.10",
    "phantomjs": "^2.1.7"
  }
}