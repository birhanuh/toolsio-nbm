{
  parser: "babel-eslint",
  "rules": {
    "prop-types": [0],
    "no-unused-vars": [1],
    "no-undef": [0],
    "no-console": [0],
    "no-case-declarations": [1],
    "no-constant-condition": [1],
    "react/no-children-prop": [0],
    "no-irregular-whitespace": [0]
  },
  "extends": ["eslint:recommended"]
}