{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "jquery": "^3.2.1",
    "jquery.easing": "^1.4.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-country-region-selector": "^1.1.0",
    "react-datepicker": "^0.46.0",
    "react-dom": "^15.4.2",
    "react-localization": "0.0.16",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0-beta.8",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-checkbox": "^2.2.11",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-dimmer": "^2.2.12",
    "semantic-ui-dropdown": "^2.2.11",
    "semantic-ui-modal": "^2.2.12",
    "semantic-ui-react": "^0.68.3",
    "semantic-ui-sidebar": "^2.2.10",
    "semantic-ui-transition": "^2.2.10",
    "semantic-ui-visibility": "^2.2.10",
    "shortid": "^2.2.8",
    "validator": "^7.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^2.0.0",
    "jest": "^21.1.0",
    "node-sass-chokidar": "0.0.1",
    "npm-run-all": "^4.0.2",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-scripts": "0.9.5",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "^1.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "proxy": "http://localhost:8080"
}
