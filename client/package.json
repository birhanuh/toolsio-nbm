  "scripts": {
    "start": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "webpack-profile": "npx webpack --mode production --profile --json > stats.json",
    "test:server:background": "npm start &",
    "//": [
      "prebuild rm -rf ./dist",
      "postbuild mv ./config/dist ./ && cp ./public/favicon.ico ./dist && cp -r ./public/error_pages ./dist"
    ],
    "postbuild": "cp ./public/favicon.ico ./config/dist && cp -r ./public/error_pages ./config/dist",
    "test": "jest './__test__'",
    "test:watch": "npm test -- --watch",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "stop:test:server:background": "pkill -f npm start"
